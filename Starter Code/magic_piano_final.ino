void setup() {
    // Note setup
    pinMode(C_NOTE,INPUT);
    pinMode(D_NOTE,INPUT);
    pinMode(E_NOTE,INPUT);
    pinMode(G_NOTE,INPUT);
    pinMode(A_NOTE,INPUT);
    pinMode(B_NOTE,INPUT);
    // 8x8 LED Setup
    lc.shutdown(0,false);
    lc.setIntensity(0,8);
    lc.clearDisplay(0);
    while(songChoice == 0){
      if(digitalRead(G_NOTE) == HIGH) {
        songChoice = 1;
      }
      if(digitalRead(A_NOTE) == HIGH) {
        songChoice = 2;
      }
      if(digitalRead(B_NOTE) == HIGH) {
        songChoice = 3;
      }
      if(digitalRead(C_NOTE) == HIGH) {
        songChoice = 4;
      }
      if(digitalRead(D_NOTE) == HIGH) {
        songChoice = 5;
      }
      if(digitalRead(E_NOTE) == HIGH) {
        songChoice = 6;
      }
    }
}

